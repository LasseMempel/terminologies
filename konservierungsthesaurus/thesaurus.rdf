  <rdf:Description rdf:about="https://www.w3id.org/archlink/terms/conservationthesaurus/DF7826">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:notation>DF7826</skos:notation>
    <skos:prefLabel xml:lang="de">Magmatit</skos:prefLabel>
    <skos:altLabel xml:lang="de">magmatisches Gestein</skos:altLabel>
    <skos:altLabel xml:lang="de">Erstarrungsgestein</skos:altLabel>
    <skos:definition xml:lang="de">Überbegriff für Gesteine, die aus Erstarrung einer Gesteinsschmelze entstehen.</skos:definition>
    <skos:broader rdf:resource="https://www.w3id.org/archlink/terms/conservationthesaurus/D866AD"/>
    <skos:note xml:lang="de">R. Dietrich/ B. Skinner, Die Gesteine und ihre Mineralien. Ein Einführungs- und Bestimmungsbuch2 (Thun 1995), S. 124.</skos:note>
    <skos:note xml:lang="de">DIN EN 12670</skos:note>
    <skos:inScheme rdf:resource="https://www.w3id.org/archlink/terms/conservationthesaurus"/>
    <skos:narrower rdf:resource="https://www.w3id.org/archlink/terms/conservationthesaurus/B14489"/>
    <skos:narrower rdf:resource="https://www.w3id.org/archlink/terms/conservationthesaurus/F646B4"/>
    <skos:narrower rdf:resource="https://www.w3id.org/archlink/terms/conservationthesaurus/CDB814"/>
    <skos:narrower rdf:resource="https://www.w3id.org/archlink/terms/conservationthesaurus/D9B246"/>
