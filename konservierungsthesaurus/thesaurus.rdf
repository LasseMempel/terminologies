  <rdf:Description rdf:about="https://www.w3id.org/archlink/terms/conservationthesaurus/G23692">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:notation>G23692</skos:notation>
    <skos:prefLabel xml:lang="de">geschrumpft</skos:prefLabel>
    <skos:altLabel xml:lang="de">Schwindung</skos:altLabel>
    <skos:definition xml:lang="de">Das Objekt zeigt eine Verkleinerung seiner Abmessungen im Vergleich zu seinen ursprünglichen Maßen.</skos:definition>
    <skos:broader rdf:resource="https://www.w3id.org/archlink/terms/conservationthesaurus/B224CD"/>
    <skos:note xml:lang="de">DIN CEN/TS 17135:2021-02, Erhaltung des kulturellen Erbes - Allgemeine Begriffe zur Beschreibung von Veränderungen an Objekten; Dreisprachige Fassung CEN/TS 17135:2020, Deutsches Institut für Normung.</skos:note>
    <skos:inScheme rdf:resource="https://www.w3id.org/archlink/terms/conservationthesaurus"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://www.w3id.org/archlink/terms/conservationthesaurus/G98FD4">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:notation>G98FD4</skos:notation>
    <skos:prefLabel xml:lang="de">stabilisierende Konservierung</skos:prefLabel>
    <skos:prefLabel xml:lang="en">conservation</skos:prefLabel>
    <skos:altLabel xml:lang="de">stabilisierende Maßnahme</skos:altLabel>
    <skos:altLabel xml:lang="en">remedial conservation</skos:altLabel>
    <skos:definition xml:lang="de">Sammelbegriff für Methoden des aktive Eingriffs, der direkt an einem Objekt vorgenommen wird, um weiteren Abbau, Zerfall und/oder Schädigung zu verhindern, zu verzögern und/oder zu begrenzen</skos:definition>
    <skos:broader rdf:resource="https://www.w3id.org/archlink/terms/conservationthesaurus/D32A26"/>
    <skos:closeMatch rdf:resource="http://vocab.getty.edu/aat/300404519"/>
    <skos:relatedMatch rdf:resource="https://www.wikidata.org/wiki/Q46993143"/>
    <skos:relatedMatch rdf:resource="https://www.wikidata.org/wiki/Q1479406"/>
    <skos:relatedMatch rdf:resource="https://d-nb.info/gnd/4114279-2"/>
    <skos:note xml:lang="de">DIN EN 15898:2020-02, Erhaltung des kulturellen Erbes - Allgemeine Begriffe; Dreisprachige Fassung EN 15898:2019, Deutsches Institut für Normung.</skos:note>
    <skos:note xml:lang="de">ICOM-CC, Terminology to characterize the conservation of tangible cultural heritage, (2008). https://www.icom-cc.org/en/terminology-for-conservation (aufgerufen am 31.10.2024)</skos:note>
    <skos:inScheme rdf:resource="https://www.w3id.org/archlink/terms/conservationthesaurus"/>
    <skos:narrower rdf:resource="https://www.w3id.org/archlink/terms/conservationthesaurus/C6A532"/>
    <skos:narrower rdf:resource="https://www.w3id.org/archlink/terms/conservationthesaurus/F72743"/>
    <skos:narrower rdf:resource="https://www.w3id.org/archlink/terms/conservationthesaurus/DA9CGG"/>
    <skos:narrower rdf:resource="https://www.w3id.org/archlink/terms/conservationthesaurus/GF41DG"/>
    <skos:narrower rdf:resource="https://www.w3id.org/archlink/terms/conservationthesaurus/AF1F5C"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://www.w3id.org/archlink/terms/conservationthesaurus/D1GC4D">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:notation>D1GC4D</skos:notation>
    <skos:prefLabel xml:lang="de">Alabaster</skos:prefLabel>
    <skos:definition xml:lang="de">Varietät des Gipses mit fester und feinkörniger Struktur und in der Regel heller bis weißer Farbe. Im allgemeinen Sprachgebrauch wird auch eine sehr ähnlich aussehende Varietät des Calcit (Kalksinter) als Alabaster ("antiker od. ägyptischer Alabaster") bezeichnet. Echter Alabaster aus Calciumsulfat ist sehr weich und anfällig gegenüber Wasser, während Calcit-Alabaster wasserunlöslich und härter ist.</skos:definition>
    <skos:broader rdf:resource="https://www.w3id.org/archlink/terms/conservationthesaurus/D8B5C3"/>
    <skos:closeMatch rdf:resource="https://cameo.mfa.org/wiki/Alabaster"/>
    <skos:closeMatch rdf:resource="https://cameo.mfa.org/wiki/Egyptian_alabaster"/>
    <skos:note xml:lang="de">DIN EN 12670</skos:note>
    <skos:note xml:lang="de">https://www.mineralienatlas.de/lexikon/index.php/MineralData?mineral=Alabaster</skos:note>
    <skos:note xml:lang="de">Günther Mehling. Naturstein-Lexikon (München 1981), S. 17.</skos:note>
    <skos:note xml:lang="de">https://www.mindat.org/min-90.html</skos:note>
    <skos:inScheme rdf:resource="https://www.w3id.org/archlink/terms/conservationthesaurus"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://www.w3id.org/archlink/terms/conservationthesaurus/AC16G1">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:notation>AC16G1</skos:notation>
    <skos:prefLabel xml:lang="de">Verpacken</skos:prefLabel>
    <skos:altLabel xml:lang="de">Verpackung</skos:altLabel>
    <skos:definition xml:lang="de">Maßnahme zur schützenden/sichernden Umhüllung des Objektes während eines Transportes und/oder der Depotlagerung</skos:definition>
    <skos:broader rdf:resource="https://www.w3id.org/archlink/terms/conservationthesaurus/F5G576"/>
    <skos:related rdf:resource="https://www.w3id.org/archlink/terms/conservationthesaurus/D9G356"/>
    <skos:note xml:lang="de">DIN EN 16141:2013-01, Erhaltung des kulturellen Erbes - Richtlinien für den Umgang mit Umwelt- und Umgebungsbedingungen - Schaudepots: Definitionen und Merkmale von Sammlungszentren bestimmt für die Bewahrung und Pflege des kulturellen Erbes; Deutsche Fassung EN 16141:2012, Deutsches Institut für Normung.</skos:note>
    <skos:inScheme rdf:resource="https://www.w3id.org/archlink/terms/conservationthesaurus"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://www.w3id.org/archlink/terms/conservationthesaurus/F94C34">
