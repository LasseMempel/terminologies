    <skos:narrower rdf:resource="https://www.w3id.org/archlink/terms/conservationthesaurus/A379F4"/>
    <skos:narrower rdf:resource="https://www.w3id.org/archlink/terms/conservationthesaurus/F968G3"/>
    <skos:narrower rdf:resource="https://www.w3id.org/archlink/terms/conservationthesaurus/GBDG4D"/>
    <skos:narrower rdf:resource="https://www.w3id.org/archlink/terms/conservationthesaurus/DD3A3D"/>
