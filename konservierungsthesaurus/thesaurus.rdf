  <rdf:Description rdf:about="https://www.w3id.org/archlink/terms/conservationthesaurus/BABA94">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:notation>BABA94</skos:notation>
    <skos:prefLabel xml:lang="de">geschweißt</skos:prefLabel>
    <skos:altLabel xml:lang="de">Schweißung</skos:altLabel>
    <skos:altLabel xml:lang="de">Verschweißung</skos:altLabel>
    <skos:altLabel xml:lang="de">verschweißt</skos:altLabel>
    <skos:definition xml:lang="de">stoffschlüssige Fügetechnik, bei der Materialien mit annähernd gleichem Schmelzintervall durch Wärme und/oder Druck in einen flüssigen oder plastischen Zustand versetzt werden, um sie miteinander zu vereinigen. Dabei kann mit oder ohne Zusatzwerkstoffen oder Flussmitteln gearbeitet werden.</skos:definition>
    <skos:broader rdf:resource="https://www.w3id.org/archlink/terms/conservationthesaurus/A15F83"/>
    <skos:note xml:lang="de">Erhard Brepohl, Theorie und Praxis des Goldschmieds (München 2000), S. 351</skos:note>
    <skos:note xml:lang="de">Technisch-wissenschaftliche Grundlagen des Goldschmiedens Teil 1. Grundtechniken der Herstellung von Schmuck und Gerät (Wiesbaden 2016), S. 148.</skos:note>
    <skos:inScheme rdf:resource="https://www.w3id.org/archlink/terms/conservationthesaurus"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://www.w3id.org/archlink/terms/conservationthesaurus/GF9C49">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:notation>GF9C49</skos:notation>
    <skos:prefLabel xml:lang="de">technische Geräte</skos:prefLabel>
    <skos:definition xml:lang="de">Oberbegriff für komplexe, technische Hilfsmittel mit elektrischen oder elektronischen Komponenten, die durch Strom betrieben werden.</skos:definition>
    <skos:broader rdf:resource="https://www.w3id.org/archlink/terms/conservationthesaurus/C93638"/>
    <rdfs:seeAlso>Janey Cronyn, The Elements of Archaeological Conservation (London 1990), S.65.</rdfs:seeAlso>
    <skos:inScheme rdf:resource="https://www.w3id.org/archlink/terms/conservationthesaurus"/>
    <skos:narrower rdf:resource="https://www.w3id.org/archlink/terms/conservationthesaurus/G487C2"/>
    <skos:narrower rdf:resource="https://www.w3id.org/archlink/terms/conservationthesaurus/D8G747"/>
    <skos:narrower rdf:resource="https://www.w3id.org/archlink/terms/conservationthesaurus/C4C6B6"/>
    <skos:narrower rdf:resource="https://www.w3id.org/archlink/terms/conservationthesaurus/C5A318"/>
    <skos:narrower rdf:resource="https://www.w3id.org/archlink/terms/conservationthesaurus/C871CB"/>
    <skos:narrower rdf:resource="https://www.w3id.org/archlink/terms/conservationthesaurus/BBB987"/>
