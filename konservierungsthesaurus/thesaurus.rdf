    <skos:notation>CD952B</skos:notation>
    <skos:prefLabel xml:lang="de">Eindellung</skos:prefLabel>
    <skos:altLabel xml:lang="de">Delle</skos:altLabel>
    <skos:definition xml:lang="de">flächige mehr oder weniger gleichmäßige Einsekung des Materials ohne Bruch.</skos:definition>
    <skos:broader rdf:resource="https://www.w3id.org/archlink/terms/conservationthesaurus/C78A4B"/>
    <skos:inScheme rdf:resource="https://www.w3id.org/archlink/terms/conservationthesaurus"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://www.w3id.org/archlink/terms/conservationthesaurus/C188DA">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:notation>C188DA</skos:notation>
    <skos:prefLabel xml:lang="de">Substanzverlust</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Loss of substance</skos:prefLabel>
    <skos:definition xml:lang="de">Material bzw. Teile des Objektes fehlen teilweise oder vollständig infolge physikalischer, chemischer, biologischer oder mechanischer Prozesse.</skos:definition>
    <skos:broader rdf:resource="https://www.w3id.org/archlink/terms/conservationthesaurus/DAC996"/>
    <skos:note xml:lang="de">DIN CEN/TS 17135:2021-02, Erhaltung des kulturellen Erbes - Allgemeine Begriffe zur Beschreibung von Veränderungen an Objekten; Dreisprachige Fassung CEN/TS 17135:2020, Deutsches Institut für Normung.</skos:note>
    <skos:inScheme rdf:resource="https://www.w3id.org/archlink/terms/conservationthesaurus"/>
