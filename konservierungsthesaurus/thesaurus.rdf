    <skos:notation>GB8544</skos:notation>
    <skos:prefLabel xml:lang="de">lackiert</skos:prefLabel>
    <skos:altLabel xml:lang="de">Lackierung</skos:altLabel>
    <skos:broader rdf:resource="https://www.w3id.org/archlink/terms/conservationthesaurus/F396G6"/>
    <skos:inScheme rdf:resource="https://www.w3id.org/archlink/terms/conservationthesaurus"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://www.w3id.org/archlink/terms/conservationthesaurus/A26FD8">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:notation>A26FD8</skos:notation>
    <skos:prefLabel xml:lang="de">aufgetieft</skos:prefLabel>
    <skos:altLabel xml:lang="de">Auftiefen</skos:altLabel>
    <skos:altLabel xml:lang="de">Tiefen</skos:altLabel>
    <skos:definition xml:lang="de">Treibtechnik, bei der ein Blech in eine mehr oder weniger passende Gegenform (z.B. Stahlanke, Kitt, Holz, etc.) eingehämmert und auf diese Weise kalt umgeformt wird.</skos:definition>
    <skos:broader rdf:resource="https://www.w3id.org/archlink/terms/conservationthesaurus/D18BBG"/>
    <skos:note xml:lang="de">Erhard Brepohl, Theorie und Praxis des Goldschmieds (München 2000), S. 268, 271.</skos:note>
    <skos:note xml:lang="de">Technisch-wissenschaftliche Grundlagen des Goldschmiedens Teil 1. Grundtechniken der Herstellung von Schmuck und Gerät (Wiesbaden 2016). Seitenzahl raussuchen!</skos:note>
    <skos:inScheme rdf:resource="https://www.w3id.org/archlink/terms/conservationthesaurus"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://www.w3id.org/archlink/terms/conservationthesaurus/AA8A54">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:notation>AA8A54</skos:notation>
    <skos:prefLabel xml:lang="de">Thiosulfat</skos:prefLabel>
