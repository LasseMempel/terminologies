    <skos:narrower rdf:resource="https://www.w3id.org/archlink/terms/conservationthesaurus/C11192"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://www.w3id.org/archlink/terms/conservationthesaurus/G967G1">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:notation>G967G1</skos:notation>
