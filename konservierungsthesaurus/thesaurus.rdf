  </rdf:Description>
  <rdf:Description rdf:about="https://www.w3id.org/archlink/terms/conservationthesaurus/C388AA">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:notation>C388AA</skos:notation>
    <skos:prefLabel xml:lang="de">Überfangguss</skos:prefLabel>
    <skos:broader rdf:resource="https://www.w3id.org/archlink/terms/conservationthesaurus/DA7G45"/>
