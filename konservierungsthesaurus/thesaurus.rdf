    <skos:inScheme rdf:resource="https://www.w3id.org/archlink/terms/conservationthesaurus"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://www.w3id.org/archlink/terms/conservationthesaurus/F37GBB">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:notation>F37GBB</skos:notation>
    <skos:prefLabel xml:lang="de">[Behandlungsmethode]</skos:prefLabel>
    <skos:broader rdf:resource="https://www.w3id.org/archlink/terms/conservationthesaurus/CFGDA3"/>
    <skos:inScheme rdf:resource="https://www.w3id.org/archlink/terms/conservationthesaurus"/>
