    <skos:notation>B2D9C9</skos:notation>
    <skos:prefLabel xml:lang="de">Mehrteilige Form</skos:prefLabel>
    <skos:definition xml:lang="de">Form, die aus mehreren Formbauteilen besteht.</skos:definition>
