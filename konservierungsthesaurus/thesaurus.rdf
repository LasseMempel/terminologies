  <rdf:Description rdf:about="https://www.w3id.org/archlink/terms/conservationthesaurus/C8CG15">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:notation>C8CG15</skos:notation>
    <skos:prefLabel xml:lang="de">Dokumentation</skos:prefLabel>
    <skos:prefLabel xml:lang="en">documentation</skos:prefLabel>
    <skos:altLabel xml:lang="en">report</skos:altLabel>
    <skos:definition xml:lang="de">Sammelbegriff für Aufzeichnungen gemachter Beobachtungen und Handlungen</skos:definition>
    <skos:topConceptOf rdf:resource="https://www.w3id.org/archlink/terms/conservationthesaurus"/>
    <skos:inScheme rdf:resource="https://www.w3id.org/archlink/terms/conservationthesaurus"/>
