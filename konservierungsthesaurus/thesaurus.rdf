  <rdf:Description rdf:about="https://www.w3id.org/archlink/terms/conservationthesaurus/CGC619">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:notation>CGC619</skos:notation>
    <skos:prefLabel xml:lang="de">Ergänzung (Restaurierungsmethode)</skos:prefLabel>
    <skos:prefLabel xml:lang="en">filling</skos:prefLabel>
    <skos:altLabel xml:lang="de">Ergänzen</skos:altLabel>
    <skos:definition xml:lang="de">Methode zur Schließung von Fehlstellen und/oder Vervollständigung fehlender Objektteile durch das Hinzufügen von Material. Wird nach der modernen archäologischen Restaurierungsethik hauptsächlich aus statischen/stabilisierenden Gründen oder zu Rekonstruktion eines vollkommen unkenntlichen Erscheinungsbildes durchgeführt.</skos:definition>
    <skos:broader rdf:resource="https://www.w3id.org/archlink/terms/conservationthesaurus/C79561"/>
    <skos:related rdf:resource="https://www.w3id.org/archlink/terms/conservationthesaurus/B7B8B6"/>
    <skos:related rdf:resource="https://www.w3id.org/archlink/terms/conservationthesaurus/FBFFF5"/>
    <skos:inScheme rdf:resource="https://www.w3id.org/archlink/terms/conservationthesaurus"/>
    <skos:narrower rdf:resource="https://www.w3id.org/archlink/terms/conservationthesaurus/GAD222"/>
    <skos:narrower rdf:resource="https://www.w3id.org/archlink/terms/conservationthesaurus/CBDC4A"/>
    <skos:narrower rdf:resource="https://www.w3id.org/archlink/terms/conservationthesaurus/A6F2C7"/>
    <skos:narrower rdf:resource="https://www.w3id.org/archlink/terms/conservationthesaurus/BD119A"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://www.w3id.org/archlink/terms/conservationthesaurus/FGA46A">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:notation>FGA46A</skos:notation>
    <skos:prefLabel xml:lang="de">Edelmetall</skos:prefLabel>
    <skos:prefLabel xml:lang="en">nobel metal</skos:prefLabel>
    <skos:altLabel xml:lang="de">Edelmetalle</skos:altLabel>
