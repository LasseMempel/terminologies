  <rdf:Description rdf:about="https://www.w3id.org/archlink/terms/conservationthesaurus/F8F594">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:notation>F8F594</skos:notation>
    <skos:prefLabel xml:lang="de">Herstellungstechnik</skos:prefLabel>
    <skos:altLabel xml:lang="de">Produktionstechnik</skos:altLabel>
    <skos:definition xml:lang="de">basierend auf den festgestellten Spuren angenommene Art und Weise wie das Objekt hergestellt worden ist</skos:definition>
