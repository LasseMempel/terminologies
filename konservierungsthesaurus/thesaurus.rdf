  <rdf:Description rdf:about="https://www.w3id.org/archlink/terms/conservationthesaurus/BGG5A6">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:notation>BGG5A6</skos:notation>
    <skos:prefLabel xml:lang="de">Transportbedingungen</skos:prefLabel>
