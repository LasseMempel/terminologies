  <rdf:Description rdf:about="https://www.w3id.org/archlink/terms/conservationthesaurus/BA75DG">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:notation>BA75DG</skos:notation>
    <skos:prefLabel xml:lang="de">salzausblühend</skos:prefLabel>
    <skos:altLabel xml:lang="de">Salzausblühungen</skos:altLabel>
    <skos:altLabel xml:lang="de">salzbelastet</skos:altLabel>
    <skos:altLabel xml:lang="de">Effloreszenz</skos:altLabel>
    <skos:altLabel xml:lang="de">Kristallbildung</skos:altLabel>
    <skos:definition xml:lang="de">Kristalline Ausfällungen von Salzen auf oder im Objekt</skos:definition>
    <skos:broader rdf:resource="https://www.w3id.org/archlink/terms/conservationthesaurus/B86G16"/>
