  <rdf:Description rdf:about="https://www.w3id.org/archlink/terms/conservationthesaurus/B9A8BF">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:notation>B9A8BF</skos:notation>
    <skos:prefLabel xml:lang="de">Bergkristall</skos:prefLabel>
    <skos:prefLabel xml:lang="en">rock crystal</skos:prefLabel>
    <skos:altLabel xml:lang="en">alaska diamond</skos:altLabel>
    <skos:definition xml:lang="de">transparente, farblose Varietät des reinen Quarz mit einer Mohs´schen Härte von 7. Charateristisch ist sein glasartiger Glanz, eine hohe Reinheit und eine ausgezeichnete Lichtdurchlässigkeit. Er bricht muschelig und zeigt keine Fluoreszenz unter UV-Licht.</skos:definition>
    <skos:broader rdf:resource="https://www.w3id.org/archlink/terms/conservationthesaurus/C84834"/>
    <skos:note xml:lang="de">https://www.mineralienatlas.de/lexikon/index.php/Mineralienportrait/Quarz/Bergkristall</skos:note>
    <skos:note xml:lang="de">https://www.mineralienatlas.de/lexikon/index.php/MineralData?mineral=Bergkristall</skos:note>
    <skos:inScheme rdf:resource="https://www.w3id.org/archlink/terms/conservationthesaurus"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://www.w3id.org/archlink/terms/conservationthesaurus/C3A182">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:notation>C3A182</skos:notation>
    <skos:prefLabel xml:lang="de">Nachbildung für wissenschaftliche Zwecke</skos:prefLabel>
    <skos:altLabel xml:lang="de">Nachbilden</skos:altLabel>
    <skos:altLabel xml:lang="de">Kopieren</skos:altLabel>
    <skos:definition xml:lang="de">Erstellung aussagekräftiger Vergleichsstücke zu wissenschaftlichem Zweck</skos:definition>
