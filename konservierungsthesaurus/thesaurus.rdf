  <rdf:Description rdf:about="https://www.w3id.org/archlink/terms/conservationthesaurus/BFAC52">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:notation>BFAC52</skos:notation>
    <skos:prefLabel xml:lang="de">Tiefenriss</skos:prefLabel>
    <skos:definition xml:lang="de">Das Objekt zeigt mindestens einen Riss, der bis tief in das Material hineinreicht</skos:definition>
    <skos:broader rdf:resource="https://www.w3id.org/archlink/terms/conservationthesaurus/FA14C2"/>
    <skos:inScheme rdf:resource="https://www.w3id.org/archlink/terms/conservationthesaurus"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://www.w3id.org/archlink/terms/conservationthesaurus/D84847">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:notation>D84847</skos:notation>
    <skos:prefLabel xml:lang="de">Wattepads</skos:prefLabel>
    <skos:broader rdf:resource="https://www.w3id.org/archlink/terms/conservationthesaurus/D941BB"/>
    <skos:inScheme rdf:resource="https://www.w3id.org/archlink/terms/conservationthesaurus"/>
