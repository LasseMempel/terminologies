  <rdf:Description rdf:about="https://www.w3id.org/archlink/terms/conservationthesaurus/B3BCB6">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:notation>B3BCB6</skos:notation>
    <skos:prefLabel xml:lang="de">Almandin</skos:prefLabel>
    <skos:definition xml:lang="de">häufigstes Mineral der Granatgruppe mit chemischer Zusammensetzung Fe3Al2(SiO4)3. Almandin zeichnet sich durch eine tiefrote bis violettrote Farbe aus, eine hohe Härte (6,5–7,5 auf der Mohs-Skala) und seine typischen isometrischen Kristalle. Es entsteht vorwiegend in metamorphen Gesteinen wie Glimmerschiefern oder Gneisen unter hohem Druck und Temperatur. Aufgrund seiner Härte und intensiven Farbe wird Almandin seit der Antike als Schmuckstein geschätzt und auch als Schleifmittel verwendet.</skos:definition>
    <skos:broader rdf:resource="https://www.w3id.org/archlink/terms/conservationthesaurus/D74F9D"/>
    <skos:note xml:lang="de">https://www.mineralienatlas.de/lexikon/index.php/MineralData?mineral=Almandin</skos:note>
    <skos:note xml:lang="de">https://www.mineralienatlas.de/lexikon/index.php/Mineralienportrait/Granat/Almandin</skos:note>
    <skos:inScheme rdf:resource="https://www.w3id.org/archlink/terms/conservationthesaurus"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://www.w3id.org/archlink/terms/conservationthesaurus/G6A358">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:notation>G6A358</skos:notation>
    <skos:prefLabel xml:lang="de">Zinnlegierung</skos:prefLabel>
    <skos:prefLabel xml:lang="en">tin alloy</skos:prefLabel>
    <skos:definition xml:lang="de">Legierung mit Zinn als Hauptbestandteil.</skos:definition>
    <skos:broader rdf:resource="https://www.w3id.org/archlink/terms/conservationthesaurus/A82D86"/>
    <skos:related rdf:resource="https://www.w3id.org/archlink/terms/conservationthesaurus/CFC44A"/>
