  <rdf:Description rdf:about="https://www.w3id.org/archlink/terms/conservationthesaurus/F2A5A7">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:notation>F2A5A7</skos:notation>
    <skos:prefLabel xml:lang="de">Haarriss</skos:prefLabel>
    <skos:definition xml:lang="de">Das Objekt zeigt einen sehr feinen, oberflächlichen Riss, der nur wenige Millimeter tief und breit ist.</skos:definition>
