    <skos:notation>CFDD8G</skos:notation>
    <skos:prefLabel xml:lang="de">Ruß</skos:prefLabel>
    <skos:broader rdf:resource="https://www.w3id.org/archlink/terms/conservationthesaurus/G89562"/>
    <skos:inScheme rdf:resource="https://www.w3id.org/archlink/terms/conservationthesaurus"/>
