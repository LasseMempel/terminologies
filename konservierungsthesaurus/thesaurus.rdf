  <rdf:Description rdf:about="https://www.w3id.org/archlink/terms/conservationthesaurus/GG6A8B">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:notation>GG6A8B</skos:notation>
    <skos:prefLabel xml:lang="de">Blattvergoldung</skos:prefLabel>
    <skos:altLabel xml:lang="de">Folienvergoldung</skos:altLabel>
