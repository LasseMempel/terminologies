    <skos:notation>A64281</skos:notation>
    <skos:prefLabel xml:lang="de">Technovit 2000 LC</skos:prefLabel>
    <skos:definition xml:lang="de">Produktname eines lichthärtendes Methacrylharzes der Firma Heraeus Kulzer GmbH und Co. KG</skos:definition>
    <skos:broader rdf:resource="https://www.w3id.org/archlink/terms/conservationthesaurus/GFD582"/>
    <skos:inScheme rdf:resource="https://www.w3id.org/archlink/terms/conservationthesaurus"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://www.w3id.org/archlink/terms/conservationthesaurus/F432G4">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:notation>F432G4</skos:notation>
    <skos:prefLabel xml:lang="de">Tyvek</skos:prefLabel>
