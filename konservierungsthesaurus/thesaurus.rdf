  <rdf:Description rdf:about="https://www.w3id.org/archlink/terms/conservationthesaurus/AA47GG">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:notation>AA47GG</skos:notation>
    <skos:prefLabel xml:lang="de">Mohairwolle</skos:prefLabel>
    <skos:altLabel xml:lang="de">Mohair</skos:altLabel>
