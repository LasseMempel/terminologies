  <rdf:Description rdf:about="https://www.w3id.org/archlink/terms/conservationthesaurus/AG4634">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:notation>AG4634</skos:notation>
    <skos:prefLabel xml:lang="de">Jute</skos:prefLabel>
    <skos:definition xml:lang="de">natürliche Stengelfaser, die aus den Pflanzen der Corchorusarten (Corchorus capsularis und Corchorus olitorius)  gewonnen wird.</skos:definition>
    <skos:broader rdf:resource="https://www.w3id.org/archlink/terms/conservationthesaurus/DD9C25"/>
    <skos:note xml:lang="de">DIN EN ISO 6938</skos:note>
    <skos:note xml:lang="de">CIETA Vocabularies - Vokabular der Textiltechniken</skos:note>
