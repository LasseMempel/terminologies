  <rdf:Description rdf:about="https://www.w3id.org/archlink/terms/conservationthesaurus/F7G7CB">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:notation>F7G7CB</skos:notation>
    <skos:prefLabel xml:lang="de">Einteilige Form</skos:prefLabel>
