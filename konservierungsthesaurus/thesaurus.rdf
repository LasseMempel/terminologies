    <skos:narrower rdf:resource="https://www.w3id.org/archlink/terms/conservationthesaurus/FGB641"/>
    <skos:narrower rdf:resource="https://www.w3id.org/archlink/terms/conservationthesaurus/G87689"/>
    <skos:narrower rdf:resource="https://www.w3id.org/archlink/terms/conservationthesaurus/CD952B"/>
