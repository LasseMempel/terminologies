  <rdf:Description rdf:about="https://www.w3id.org/archlink/terms/conservationthesaurus/DF1279">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:notation>DF1279</skos:notation>
    <skos:prefLabel xml:lang="de">Speckstein</skos:prefLabel>
    <skos:altLabel xml:lang="de">Steatit</skos:altLabel>
    <skos:definition xml:lang="de">metamorphoses Gestein, das nahezu ausschließlich aus dem Mineral Talk besteht. Speckstein zeichnet sich, aufgrund des hohen Talkgehaltes, durch seine Weichheit, leichte Bearbeitbarkeit und eine glatte, fettige Oberfläche aus. Die Farbe ist meist lichtgrau.</skos:definition>
    <skos:broader rdf:resource="https://www.w3id.org/archlink/terms/conservationthesaurus/ABB166"/>
    <skos:note xml:lang="de">R. Dietrich/ B. Skinner, Die Gesteine und ihre Mineralien. Ein Einführungs- und Bestimmungsbuch2 (Thun 1995), S. 308/309.</skos:note>
    <skos:inScheme rdf:resource="https://www.w3id.org/archlink/terms/conservationthesaurus"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://www.w3id.org/archlink/terms/conservationthesaurus/G43DBC">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:notation>G43DBC</skos:notation>
    <skos:prefLabel xml:lang="de">beschädigt</skos:prefLabel>
    <skos:altLabel xml:lang="de">Beschädigung</skos:altLabel>
    <skos:altLabel xml:lang="de">Schaden</skos:altLabel>
    <skos:definition xml:lang="de">Das Objekt weist eine ungewollte, negative Veränderung im Vergleich zu seinem ursprünglichen Aussehen auf, die die wissenschaftliche Aussagekraft des Objektes mindert.</skos:definition>
    <skos:broader rdf:resource="https://www.w3id.org/archlink/terms/conservationthesaurus/C6GB1C"/>
    <skos:note xml:lang="de">DIN EN 15898:2020-02, Erhaltung des kulturellen Erbes - Allgemeine Begriffe; Dreisprachige Fassung EN 15898:2019, Deutsches Institut für Normung.</skos:note>
    <skos:inScheme rdf:resource="https://www.w3id.org/archlink/terms/conservationthesaurus"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://www.w3id.org/archlink/terms/conservationthesaurus/AGBB3C">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:notation>AGBB3C</skos:notation>
    <skos:prefLabel xml:lang="de">Festigkeit</skos:prefLabel>
    <skos:definition xml:lang="de">Fähigkeit des Objektwerkstoffs mechanischen Widerstand gegen eine plastische Verformung oder Trennung zu leisten</skos:definition>
