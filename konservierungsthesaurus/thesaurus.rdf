  <rdf:Description rdf:about="https://www.w3id.org/archlink/terms/conservationthesaurus/C5GA26">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:notation>C5GA26</skos:notation>
    <skos:prefLabel xml:lang="de">Reproduktion (Objekt)</skos:prefLabel>
    <skos:definition xml:lang="de">Objekt, welches nach Vorbild eines von einer anderen Person angefertigten Originals hergestellt wurde, ohne das dieses Original im Vorfeld abgeformt wurde</skos:definition>
    <skos:broader rdf:resource="https://www.w3id.org/archlink/terms/conservationthesaurus/C11192"/>
