    <skos:narrower rdf:resource="https://www.w3id.org/archlink/terms/conservationthesaurus/D38147"/>
    <skos:narrower rdf:resource="https://www.w3id.org/archlink/terms/conservationthesaurus/BB9249"/>
    <skos:narrower rdf:resource="https://www.w3id.org/archlink/terms/conservationthesaurus/F5G8DB"/>
    <skos:narrower rdf:resource="https://www.w3id.org/archlink/terms/conservationthesaurus/A99485"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://www.w3id.org/archlink/terms/conservationthesaurus/G4AG5C">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:notation>G4AG5C</skos:notation>
    <skos:prefLabel xml:lang="de">Edelmetalllegierung</skos:prefLabel>
    <skos:definition xml:lang="de">Legierung, bei dem der Hauptbestandteil ein Edelmetall ist.</skos:definition>
