  <rdf:Description rdf:about="https://www.w3id.org/archlink/terms/conservationthesaurus/GFDB57">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:notation>GFDB57</skos:notation>
    <skos:prefLabel xml:lang="de">Stroh</skos:prefLabel>
    <skos:prefLabel xml:lang="en">straw</skos:prefLabel>
    <skos:definition xml:lang="de">getrocknete Halme pflanzlicher Fasern, die für eine Vielzahl unterschiedlicher Zwecke verwendet werden, z.B. zum Weben, Flechten oder als Magerungsmittel für Ton</skos:definition>
